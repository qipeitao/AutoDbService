<Window x:Class="AutoDbService.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoDbService.Wpf"
        mc:Ignorable="d"
        xmlns:prism="http://prismlibrary.com/" 
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <ContentControl Grid.Column="1" prism:RegionManager.RegionName="MainRegion">
            <Button Content="Button" Command="{Binding AddCommand}" DataContext="{Binding MM}" HorizontalAlignment="Left" Margin="261,182,0,0" VerticalAlignment="Top"/>
        </ContentControl>
    </Grid>
</Window>
